# Example environment variables for Benchpress
# Copy this file to .env and fill in your values

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GLHF_API_KEY=your_glhf_api_key_here

# API Bases (optional)
# OPENAI_API_BASE=https://custom-openai-endpoint.com
# GLHF API base is set by default to https://glhf.chat/api/openai/v1

# Data paths
BENCHPRESS_DATA_PATH=./data